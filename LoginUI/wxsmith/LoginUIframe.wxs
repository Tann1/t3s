<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="LoginUIFrame">
		<title>UIFrame</title>
		<centered>1</centered>
		<size>350,400</size>
		<minsize>350,400</minsize>
		<maxsize>350,400</maxsize>
		<id_arg>0</id_arg>
		<style>wxDEFAULT_FRAME_STYLE|wxRESIZE_BORDER</style>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxPanel" name="PANEL_LOGIN" variable="LoginUI_Panel" member="yes">
			<size>350,400</size>
			<bg>#E94B3C</bg>
			<minsize>350,400</minsize>
			<maxsize>350,400</maxsize>
			<object class="wxStaticText" name="Text_Header" variable="ST_Header" member="yes">
				<label>Meal Variety &#x0A;Prepping</label>
				<pos>75,24</pos>
				<size>120,16</size>
				<fg>wxSYS_COLOUR_MENU</fg>
				<font>
					<size>24</size>
					<style>normal</style>
					<weight>bold</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Ofelia Display</face>
				</font>
				<style>wxALIGN_CENTRE</style>
			</object>
			<object class="wxTextCtrl" name="TEXTCTRL_USERNAME" variable="TC_Username" member="yes">
				<pos>128,144</pos>
				<size>100,25</size>
			</object>
			<object class="wxTextCtrl" name="TEXTCTRL_PASSWORD" variable="TC_Password" member="yes">
				<pos>128,176</pos>
				<size>100,25</size>
				<maxsize>-1,-1</maxsize>
				<style>wxTE_PASSWORD</style>
			</object>
			<object class="wxButton" name="ID_BUTTON1" variable="B_Login" member="yes">
				<label>Login</label>
				<pos>128,208</pos>
				<size>100,25</size>
				<font>
					<size>10</size>
					<style>normal</style>
					<weight>bold</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Ofelia Display</face>
				</font>
				<style>wxBU_BOTTOM|wxBORDER_NONE</style>
				<handler function="OnB_LoginClick" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="BUTTON_SIGNUP" variable="B_Signup" member="yes">
				<label>Sign up</label>
				<pos>128,238</pos>
				<size>100,25</size>
				<font>
					<size>10</size>
					<style>normal</style>
					<weight>bold</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Ofelia Display</face>
				</font>
				<style>wxBU_BOTTOM|wxBORDER_NONE</style>
				<handler function="OnB_SignupClick" entry="EVT_BUTTON" />
			</object>
			<object class="wxStaticText" name="STATICTEXT_Username" variable="ST_Username" member="yes">
				<label>Username:</label>
				<pos>40,144</pos>
				<size>75,24</size>
				<font>
					<size>11</size>
					<style>normal</style>
					<weight>normal</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Ofelia Display</face>
				</font>
			</object>
			<object class="wxStaticText" name="STATICTEXT_PASSWORD" variable="ST_Password" member="yes">
				<label>Password:</label>
				<pos>40,176</pos>
				<size>75,25</size>
				<font>
					<size>11</size>
					<style>normal</style>
					<weight>normal</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Ofelia Display</face>
				</font>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
